REGISTRY?=lcr.loongnix.cn
ORGANIZATION?=kubernetes
REPOSITORY?=etcd
TAG?=3.4.13

IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(REPOSITORY):$(TAG)


default: image

download:
	wget https://github.com/Loongson-Cloud-Community/etcd-multi/releases/download/v3.4.13/etcd-v3.4.13-linux-loong64.tar.gz && tar xf etcd-v3.4.13-linux-loong64.tar.gz

image: download
	docker build \
		-t $(IMAGE) \
		.

push:
	docker push $(IMAGE)
