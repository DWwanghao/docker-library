REGISTRY?=lcr.loongnix.cn
ORGANIZATION?=kubernetes
REPOSITORY?=etcd
TAG?=3.5.12

IMAGE=$(REGISTRY)/$(ORGANIZATION)/$(REPOSITORY):$(TAG)


default: image

download:
	wget http://cloud.loongnix.cn/releases/loongarch64/etcd-io/etcd/3.5.12/etcd-v3.5.12-linux-loong64.tar.gz && tar xf etcd-v3.5.12-linux-loong64.tar.gz

image: download
	docker build \
		-t $(IMAGE) \
		.

push:
	docker push $(IMAGE)
